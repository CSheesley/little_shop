<% @order_item_id %>

<%= form_for [:profile, @review] do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :description %>
  <%= f.text_area :description %>
</p>
<p>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
</p>
<p>
  <%= f.hidden_field :order_item_id, value: @order_item_id %>
</p>
<%= f.submit %>
<% end %>
